class FifthQuestion(QtWidgets.QMainWindow, Fifth_Question):
    def __init__(self):
        super(FifthQuestion, self).__init__()
        self.setupUi(self)

        self.pushButton.clicked.connect(self.open_question)

        self.endWindow = EndWindow()
    
    def resultresult(self):
                if test_result == 0 or test_result == 1 or test_result == 2:
                    self.endWindow.label_2.setText("Вы успешно прошли тест!\n\nВаш балл:\n\n" + str(test_result) + "/5\n\nВаша оценка:\n\n2")
                elif test_result == 3:
                    self.endWindow.label_2.setText("Вы успешно прошли тест!\n\nВаш балл:\n\n" + str(test_result) + "/5\n\nВаша оценка:\n\n3")
                elif test_result == 4:
                    self.endWindow.label_2.setText("Вы успешно прошли тест!\n\nВаш балл:\n\n" + str(test_result) + "/5\n\nВаша оценка:\n\n4")
                elif test_result == 5:
                    self.endWindow.label_2.setText("Вы успешно прошли тест!\n\nВаш балл:\n\n" + str(test_result) + "/5\n\nВаша оценка:\n\n5")

    def open_question(self):
        if self.button_varian_1.isChecked() == True or self.button_varian_2.isChecked() == True or self.button_varian_3.isChecked() == True or self.button_varian_4.isChecked() == True:
            def answer_check(self):
                    if self.button_varian_3.isChecked() == True:
                        global test_result
                        test_result += 1
                        print(test_result)
            
            answer_check(self)
            
            self.resultresult()
            self.endWindow.show()
            self.hide()

        else:
            error = QMessageBox()
            error.setWindowTitle("Ошибка")
            error.setText("Выберите вариант ответа")
            error.setIcon(QMessageBox.Warning)
            error.setStandardButtons(QMessageBox.Ok)
            error.setDefaultButton(QMessageBox.Ok)
            error.setInformativeText("Посмотрите на флаг и выберите хотя бы один вариант ответа из предложенных")
            error.exec_()


class EndWindow(QtWidgets.QMainWindow, End_Window):
    def __init__(self):
        super(EndWindow, self).__init__()
        self.setupUi(self)

        
        self.pushButton_2.clicked.connect(self.exit)
        
        self.pushButton.clicked.connect(self.repeat)

    def repeat(self):
        global test_result 
        test_result = 0
        self.next = MainWindow()
        self.next.show()
        self.hide()

    def exit(self):
        # app = QtWidgets.QApplication(sys.argv)
        # app.exec_()
        import sys
        sys.exit()
 
        
        
        
        
        

# Скрипт запуск/завершение

if __name__ == "__main__":
    for i in range(1):
        import sys
        app = QtWidgets.QApplication(sys.argv)
        w = MainWindow()
        w.show()
        sys.exit(app.exec_())
