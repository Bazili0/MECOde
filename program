class MainWindow(QtWidgets.QMainWindow, Ui_Menu):
    def __init__(self):
        super(MainWindow,self).__init__()
        self.setupUi(self)
        
        self.pushButton.clicked.connect(self.open_question)
        
        self.firstQuestion = FirstQuestion()

    def open_question(self):
        self.firstQuestion.show()
        self.hide()


class FirstQuestion(QtWidgets.QMainWindow, First_Question):
    def __init__(self):
        super(FirstQuestion, self).__init__()
        self.setupUi(self)

        self.pushButton.clicked.connect(self.open_question)

        self.secondQuestion = SecondQuestion()

    def open_question(self):
        
        if self.button_varian_1.isChecked() == True or self.button_varian_2.isChecked() == True or self.button_varian_3.isChecked() == True or self.button_varian_4.isChecked() == True:
            def answer_check(self):
                if self.button_varian_3.isChecked() == True:
                    global test_result 
                    test_result += 1
                    print(test_result)

            answer_check(self)
            self.secondQuestion.show()
            self.hide()

        else:
            error = QMessageBox()
            error.setWindowTitle("Ошибка")
            error.setText("Выберите вариант ответа")
            error.setIcon(QMessageBox.Warning)
            error.setStandardButtons(QMessageBox.Ok)
            error.setDefaultButton(QMessageBox.Ok)
            error.setInformativeText("Посмотрите на флаг и выберите хотя бы один вариант ответа из предложенных")
            error.exec_()


